
.btn-invitation{
  margin: 50rpx;
}

/* 分享成功提示框 */
.modal-invitation-success{
  width: 100%;
  height: 100%;
  background-color: rgba(15, 15, 15, 0.65);
  position: absolute;
  left: 0;
  top: 0;
}
.modal-invitation-success-box{
  width: 560rpx;
  height: 442rpx;
  position: absolute;
  left: 50%;
  margin-left: -280rpx;
  top: 50%;
  margin-top: -221rpx;
  background-color: #fff;
  border-radius: 10rpx;
}
.delete-box {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
}
.delete-btn {
  position: absolute;
  right: 30rpx;
  top: 22rpx;
  width: 25rpx;
  height: 25rpx;
}
.modal-invitation-success-img {
  width: 156rpx;
  height: 54rpx;
  margin-top: 57rpx;
  margin-left: 208rpx;
}
.modal-invitation-success-content{
  text-align: center;
  margin-top: 30rpx;
}
.dots{
  border-bottom: 3rpx dotted #f36e89;
  position: absolute;
  left: 42rpx;
  bottom: 172rpx;
  width: 481rpx;
}
.modal-invitation_notice-btn{
  position: absolute;
  bottom: 62rpx;
  left: 50%;
  margin-left: -115rpx;
  text-align: center;
  background-color: #fd5293;
  color: #F9EAED;
  width: 230rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  border: 5rpx solid #ffe8b4;
}


